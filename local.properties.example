# This file is used to store local configuration that should not be committed to version control.
# Copy this file to local.properties and fill in the values.

# SDK location (auto-generated by Android Studio, usually not needed)
# sdk.dir=/path/to/Android/sdk

# Cloudinary Configuration
# Get these from your Cloudinary dashboard at https://cloudinary.com/console
cloudinary.cloud.name=your_cloud_name_here
cloudinary.api.key=your_api_key_here
cloudinary.api.secret=your_api_secret_here
cloudinary.upload.folder=sampah-jujur

# Notification Server Configuration
# For development with Android Emulator, use http://10.0.2.2:3000
# For development with real device on same network, use http://YOUR_LOCAL_IP:3000
# For production, use your deployed server URL (e.g., https://your-server.com)
notification.server.url=http://10.0.2.2:3000
